{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/main/docs-root.ts"],
    "entryPointStrategy": "resolve",
    "out": "./docs/reference",
    "cleanOutputDir": false,
    "sort": ["static-first", "kind", "source-order"],
    "includeVersion": true,
    "emit": "both",
    "sourceLinkExternal": true,
    "cname": "clockify.siliconcosmos.com",
    "excludePrivate": true,
    "excludeReferences": false
}